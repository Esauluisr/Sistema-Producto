	<parameter name="Nombre" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Fecha1" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Fecha2" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    MAX(d.Nombre_Empleado) AS nombre_director,
    MAX(s.Nombre_Empleado) as nombre_JEFADELAOFICINADEPERSONAL,
    e.Nombre_Empleado,
    e.Puesto,
    SUBSTRING(i.Fecha, 9, 2) AS Dia,
    SUBSTRING(i.Fecha, 1, 4) AS Año,
    CASE
        WHEN MONTH(Fecha) = '01' THEN 'ENERO'
        WHEN MONTH(Fecha) = '02' THEN 'FEBRERO'
        WHEN MONTH(Fecha) = '03' THEN 'MARZO'
        WHEN MONTH(Fecha) = '04' THEN 'ABRIL'
        WHEN MONTH(Fecha) = '05' THEN 'MAYO'
        WHEN MONTH(Fecha) = '06' THEN 'JUNIO'
        WHEN MONTH(Fecha) = '07' THEN 'JULIO'
        WHEN MONTH(Fecha) = '08' THEN 'AGOSTO'
        WHEN MONTH(Fecha) = '09' THEN 'SEPTIEMBRE'
        WHEN MONTH(Fecha) = '10' THEN 'OCTUBRE'
        WHEN MONTH(Fecha) = '11' THEN 'NOVIEMBRE'
        WHEN MONTH(Fecha) = '12' THEN 'DICIEMBRE'
    END AS Mes,
    substring(GROUP_CONCAT(
        CASE
            WHEN i.Estado IS NULL THEN
                CASE
                    WHEN i.P_Incidencia = 'Falta' THEN '(x) Falta'
                    ELSE '( ) Falta'
                END
            ELSE
CASE 
					WHEN estado='Entrada' THEN '( ) Falta'
						ELSE 
				CASE
                    WHEN estado='Salida' THEN '( ) Falta'
						END 
					END    
        END
    ),1,9) AS falta,
    substring(GROUP_CONCAT(
        CASE
			when estado='Entrada' then
            case
            WHEN P_Incidencia='Normal' THEN
                 CONCAT('( ) Retardo de__horas__minutos    ') 
            ELSE 
				CASE
					WHEN SUBSTRING(P_Incidencia, 1, 2) = 'RT' then  CONCAT('(x) Retardo de', SUBSTRING(P_Incidencia, 3, 3), ' horas ', SUBSTRING(P_Incidencia, 7, 8), ' minutos')
                    else null
                    end
            end 
            else 
				CASE 
					WHEN estado is null then '( ) Retardo de__horas__minutos    '
                    else
				CASE
                    WHEN estado='Entrada' THEN '( ) Retardo de__horas__minutos    '
						ELSE 
				CASE
                    WHEN estado='Salida' and (s_incidencia='N.C.E' or s_incidencia='N.C.E')THEN '( ) Retardo de__horas__minutos    '
						END 
					END 
				END
        END
    ),1,34) AS Retardo,
    substring(GROUP_CONCAT(
        CASE
			when estado='Salida' then 
            case
            WHEN SUBSTRING(P_Incidencia, 1, 5) = 'CH.A' THEN
                CONCAT('(x) Checó ', SUBSTRING(P_Incidencia, 6, 2), ' horas ', SUBSTRING(P_Incidencia, 9, 11), ' minutos antes de su hora de salida')
            ELSE CONCAT('( ) Checó__horas__minutos antes de su hora de salida   ')
            end 
            else 
				CASE 
					WHEN estado is null then '( ) Checó__horas__minutos antes de su hora de salida   '
                    else 
						CASE 
							WHEN estado = 'Entrada'and (S_Incidencia = 'N.C.S' or 'N.C.E') then '( ) Checó__horas__minutos antes de su hora de salida   '
							else '( ) Checó__horas__minutos antes de su hora de salida   '
						end
                    end
        END
    ),1,56) AS Horas_Antes,
    Substring(GROUP_CONCAT(
        CASE
			when estado='Salida' then 
            case
            WHEN SUBSTRING(S_Incidencia, 1, 5) = 'N.C.E' THEN 'No checó:   Entrada (x)        Salida ( )'
            ELSE 
                CASE 
                    WHEN  SUBSTRING(S_Incidencia, 1, 5) = 'N.C.S' THEN 'No checó:   Entrada ( )        Salida (x)'
                    ELSE 'No checó:   Entrada ( )        Salida ( )'
                END
			end
            else 
				case
					WHEN  SUBSTRING(S_Incidencia, 1, 5) = 'N.C.S' THEN 'No checó:   Entrada ( )        Salida (x)'
                    ELSE 
						CASE 
							WHEN estado is null then 'No checó:   Entrada ( )        Salida ( )'
								else 'No checó:   Entrada ( )        Salida ( )'
						end
                END
        END
    ),1,41) AS Chequéo,
    substring(GROUP_CONCAT(
        CASE
            WHEN WEEKDAY(Fecha) = 0 THEN
                CONCAT(e.Lunes_entrada, ' a ', e.Lunes_salida, ' HRS.')
            WHEN WEEKDAY(Fecha) = 1 THEN
                CONCAT(e.Martes_entrada, ' a ', e.Martes_salida, ' HRS.')
            WHEN WEEKDAY(Fecha) = 2 THEN
                CONCAT(e.Miercoles_entrada, ' a ', e.Miercoles_salida, ' HRS.')
            WHEN WEEKDAY(Fecha) = 3 THEN
                CONCAT( e.Jueves_entrada, ' a ', e.Jueves_salida, ' HRS.')
            WHEN WEEKDAY(Fecha) = 4 THEN
                CONCAT( e.Viernes_entrada, ' a ', e.Viernes_salida, ' HRS.')
            WHEN WEEKDAY(Fecha) = 5 THEN
                CONCAT( e.Sabado_entrada, ' a ', e.Sabado_salida, ' HRS.')
            WHEN WEEKDAY(Fecha) = 6 THEN
                CONCAT( e.Domingo_entrada, ' a ', e.Domingo_salida, ' HRS.')
        END
    ),1,17) AS Horario
FROM
    incidencias_semestre i
LEFT JOIN horario_semestre e ON i.IDEmpleado = e.ID_Empleado
LEFT JOIN horario_semestre d ON d.Puesto = 'Director'
LEFT JOIN horario_semestre s ON s.Puesto = 'JEFA DE LA OFNA. DE PERSONAL'  -- Agregar un JOIN adicional para obtener al director
WHERE (i.Justificación IS NULL OR i.Justificación = 'Reporte') 
    AND ((i.P_Incidencia != 'Normal' AND i.P_Incidencia != 'NT' AND i.S_Incidencia != 'Normal') 
    OR ((i.S_Incidencia = 'Normal' OR i.S_Incidencia IS NULL) AND i.P_Incidencia != 'Normal' AND i.P_Incidencia != 'NT') 
    OR ((i.P_Incidencia = 'Normal' OR i.P_Incidencia = 'NT') AND i.S_Incidencia != 'Normal'))
    AND (Nombre COLLATE utf8_general_ci LIKE concat($P{Nombre},'%')) 
    AND i.Fecha BETWEEN STR_TO_DATE($P{Fecha1}, '%d/%m/%Y') AND STR_TO_DATE($P{Fecha2}, '%d/%m/%Y')
GROUP BY e.Nombre_Empleado, i.Fecha, e.Puesto;]]>
